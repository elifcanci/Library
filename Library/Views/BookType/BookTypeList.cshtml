@model List<BookType>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3 class="text-center">Book List Page</h3>
<table class="table table-striped">
    <thead>
        <tr>
            <th>#</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <th>@(i+1)</th>
                <td>@Model[i].Name</td>
            </tr>
        }
    </tbody>
</table>
